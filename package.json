{
  "name": "@lpezet/etl-js-cli",
  "version": "1.0.3",
  "description": "Command Line Interface for ETL-JS.",
  "main": "",
  "scripts": {
    "prepare_off": "./node_modules/marked-man/bin/marked-man --version v1.0.0 --manual 'ETL-JS CLI' ./man/Manual.md > ./man/etl-js-cli.1",
    "lint": "eslint lib/ tests/",
    "fix": "eslint lib/ tests/ --fix",
    "pretest": "npm run lint",
    "test": "nyc mocha --bail --check-leaks --reporter spec \"./tests/**/*-test.js\"",
    "posttest": "nyc check-coverage",
    "integration-test": "mocha ./integration-tests/*-test.js",
    "docs-gen": "doctoc README.md Mods.md --github --no-title",
    "test-watch": "mocha --watch --reporter=spec",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lpezet/etl-js-cli.git"
  },
  "keywords": [
    "etl"
  ],
  "files": [
    "LICENSE",
    "README.md",
    "lib/"
  ],
  "nyc": {
    "include": "lib",
    "check-coverage": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 80,
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "author": "Luke Pezet <lpezet@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lpezet/etl-js-cli/issues"
  },
  "bak": {
    "@lpezet/etl-js": "file:../etl-js"
  },
  "homepage": "https://github.com/lpezet/etl-js-cli#readme",
  "dependencies": {
    "@lpezet/etl-js": "^1.2.0",
    "commander": "^2.20.0",
    "winston": "^2.4.4",
    "yaml-cfn": "^0.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^5.0.0",
    "nyc": "^14.1.0",
    "eslint": "^6.1.0"
  }
}
